<div class=" well">
    <h3 style="text-decoration:underline; padding-left:20px;">Get page keywords</h3>
    <%= bootstrap_form_for(:seo, inline_errors: true,url:seo_post_page_keywords_path,layout: :horizontal) do |f| %>
        <%= f.text_field :site_uri  , control_col: "col-sm-8" , placeholder: "Put web site Url" %>
        <%= f.form_group do %>
            <%= f.submit "Submit", class: "btn btn-success"%>
        <% end %>
    <% end %>
</div>
