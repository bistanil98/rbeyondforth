
<div class=" well">
    <h3 style="text-decoration:underline; padding-left:20px;">Get meta tags</h3>
    <%= bootstrap_form_for(:seo, url:seo_post_meta_tags_path, layout: :horizontal) do |f| %>
        <%= f.text_field :site_uri, control_col: "col-sm-8" , class:"input-medium search-query" , value:"http://" %>
        <%= f.form_group do %>
            <%= f.submit "Submit", class: "btn btn-success"%>
            <p></p>
        <% end %>
    <% end %>
</div>


